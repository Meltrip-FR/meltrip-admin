{
  "hosting": {
    "public": "out",
    "target": "default",
    "appAssociation": "AUTO",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/auth/signin",
        "destination": "/auth/signin.html"
      },
      {
        "source": "/admin/dashboard",
        "destination": "/admin/dashboard.html"
      },
      {
        "source": "/admin/seminar",
        "destination": "/admin/seminar.html"
      },
      {
        "source": "/admin/seminar/waitinglist/",
        "destination": "/admin/seminar/waitinglist/.html"
      },
      {
        "source": "/admin/seminar/waitinglist/*",
        "destination": "/admin/seminar/waitinglist/[id].html"
      },
      {
        "source": "/500",
        "destination": "/500.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
